{
    "name": "cordoval/manager-tools",
    "description": "github maintainer manager tools",
    "license": "MIT",
    "authors": [
        {
            "name": "cordoval",
            "email": "cordoval@gmail.com"
        }
    ],
    "require": {
        "symfony/console": "2.3.*",
        "knplabs/github-api": "dev-master@dev",
        "incenteev/composer-parameter-handler": "~2.0",
        "stil/phoebe": "dev-master@dev",
        "raulfraile/ladybug-bundle": "~1.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "incenteev-parameters": {
            "file": "parameters.yml",
            "keep-outdated": true
        }
    },
    "autoload": {
        "psr-0": { "": "src" }
    }
}
