{
    "name": "cordoval/manager-tools",
    "description": "github maintainer manager tools",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.3.*",
        "knplabs/github-api": "1.2.*@dev",
        "incenteev/composer-parameter-handler": "~2.0",
        "stil/phoebe": "dev-master",
        "raulfraile/ladybug-bundle": "~1.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "incenteev-parameters": {
            "file": "parameters.yml",
            "keep-outdated": true
        }
    }
}
